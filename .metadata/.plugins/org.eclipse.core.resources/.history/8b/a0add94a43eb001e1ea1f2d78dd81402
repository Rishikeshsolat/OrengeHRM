package com.MulyilevelInheritance.constructor;
//Parent class
class Animal {
 String name;

 // Constructor to initialize name
 Animal(String name) {
     this.name = name;
 }

 void eat() {
     System.out.println(name + " is eating.");
 }
}

//Child class inheriting from Animal
class Dog extends Animal {
 // Constructor to initialize name using super keyword
 Dog(String name) {
     super(name);
 }

 void bark() {
     System.out.println(name + " is barking.");
 }
}

//Grandchild class inheriting from Dog
class Labrador extends Dog {
 // Constructor to initialize name using super keyword
 Labrador(String name) {
     super(name);
 }

 void play() {
     System.out.println(name + " is playing.");
 }
}

public class Main {
 public static void main(String[] args) {
     Labrador myLabrador = new Labrador("Buddy");

     myLabrador.eat(); // Accessing eat() method from Animal class
     myLabrador.bark(); // Accessing bark() method from Dog class
     myLabrador.play(); // Accessing play() method from Labrador class
 }
}
